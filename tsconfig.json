{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": "src",
    "rootDir": "src",
    "paths": {
      "@/*": ["*"]
    },
    "module": "esnext",
    "allowJs": true,
    "esModuleInterop": true,
    // "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx"
  }
}
